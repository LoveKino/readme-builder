{
  "name": "readme-builder",
  "version": "0.0.12",
  "description": " A simple tool used to generate read me doc for project\n[readme-lang:zh]用于生成项目的readme文档的简单工具",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec 'test/function/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoveKino/readme-builder.git"
  },
  "keywords": [
    "readme",
    "doc",
    "automatic"
  ],
  "author": "ddchen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LoveKino/readme-builder/issues"
  },
  "homepage": "https://github.com/LoveKino/readme-builder#readme",
  "dependencies": {
    "defcomment": "^0.4.12",
    "del": "^2.2.2",
    "describe-data": "0.0.1",
    "dir-tree-info": "0.0.2",
    "es6-promisify": "^5.0.0",
    "glob": "^7.1.1",
    "markdown-toc": "^1.1.0",
    "minimatch": "^3.0.3",
    "mix-lang-text": "0.0.2",
    "uuid": "^3.0.1",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "mocha": "^3.2.0"
  },
  "bin": {
    "buildreadme": "./bin/build-readme.js"
  },
